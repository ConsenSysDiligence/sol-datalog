{
    "extension": [".ts"],
    "exclude": [
        "**/*.d.ts",
        "**/node_modules/**",
        "**/test/**",
        "**/coverage/**",
        "**/.compiler_cache/**",
        "src/bin/cli.ts",
        "scripts/gen_preamble.ts"
    ],
    "reporter": ["lcov", "text-summary"],
    "all": true,
    "check-coverage": true,
    "statements": 90,
    "branches": 85,
    "functions": 90,
    "lines": 95
}
